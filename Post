			$office=~s/<[^>]*?>/ /igs;
			$phone=~s/<[^>]*?>/ /igs;
			$email=~s/<[^>]*?>/ /igs;
			$name=~s/<[^>]*?>/ /igs;
			$name=~s/\s+/ /igs;
			$url=~s/\s+/ /igs;
			$title=~s/\s+/ /igs;
			$office=~s/\s+/ /igs;
			$phone=~s/\s+/ /igs;
			$email=~s/\s+/ /igs;
			$fax=~s/\s+/ /igs;
			$practice=~s/\s+/ /igs;
			
			open(FH,">>paulweiseOutput.txt");
			print FH "$name\tt$title\t$office\t$phone\t$mail\t\t$url\n";
			close FH;

		}		
		   

	  }
	  else {
		  print $res->status_line, "\n";
	  }
	}  
